#include <iostream>
using namespace std;

class B {
	private: 
		int B1;
		float B2;
	public:
		void MB1();
		void MB2();
		void setB1(const int& var) {
			B1 = var;
		}
		int getB1() {
			return B1;
		}  
    void setB2(const int& var) {
			B2 = var;
		}
		int getB2() {
			return B2;
		}
};

void B::MB1(){
	cout << "O nome do metodo eh MB1" << endl;
}

void B::MB2(){
	cout << "O nome do metodo eh MB2" << endl;	
}

int main(){
	B meuMetodo;

	meuMetodo.MB1();
	meuMetodo.MB2();

	return 0;
}
